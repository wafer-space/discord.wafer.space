    {
      "id": "1431021703684952287",
      "type": "Default",
      "timestamp": "2025-10-23T20:49:21.256+00:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "Hi again, just to clarify, are the \"input\" pins in the template what you refer to as analog inputs?",
      "author": {
        "id": "1338991700084785204",
        "name": "jalf_47390",
        "discriminator": "0000",
        "nickname": "Alf",
        "color": null,
        "isBot": false,
        "roles": [],
        "avatarUrl": "mole99-is-there-any-esd-protection-in_media/3-FB033.png"
      },
      "attachments": [],
      "embeds": [],
      "stickers": [],
      "reactions": [],
